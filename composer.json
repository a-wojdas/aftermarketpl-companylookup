{
    "name": "aftermarketpl/companylookup",
    "description": "The package provides tool to validate any company data such as vatid, address, current vies status.",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-simplexml": "*",
        "ext-soap": "*",
        "ext-libxml": "*"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Aftermarket.pl",
            "email": "kontakt@aftermarket.pl"
        }
    ],
    "autoload": {
        "psr-4": {
            "Aftermarketpl\\CompanyLookup\\": "src/"
        }
    },
    "autoload-dev": {
        "files": ["env.php"]
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^8"
    }
}
